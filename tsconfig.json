{
  "compilerOptions": {
    "jsx": "preserve",
    "pretty": true,
    "sourceMap": true,
    "target": "es2015",
    "outDir": "lib",
    "moduleResolution": "node"
  },
  "exclude": [
    "typings/globals",
    "typings/index.d.ts",
    "node_modules",
    "dist"
  ],
  "files": [
    "./src/types/types.ts",
    "./src/utils/classnames.ts",
    "./src/utils/cookiestore.ts",
    "./src/utils/graphql.ts",
    "./src/utils/polyfils.ts",
    "./src/utils/relay.ts",
    "./src/views/models/BrowserView/utils.ts",
    "./src/components/Icon/Icon.tsx",
    "./src/components/Loading/Loading.tsx",
    "./src/components/ScrollBox/ScrollBox.tsx",
    "./src/components/ToggleButton/ToggleButton.tsx",
    "./src/views/models/BrowserView/Cell.tsx",
    "./src/views/models/BrowserView/CheckboxCell.tsx",
    "./src/views/models/StructureView/FieldPopup.tsx",
    "./src/views/models/StructureView/PermissionsLayover.tsx",
    "./src/views/models/StructureView/PermissionType.tsx",
    "./src/views/models/StructureView/TypeSelection.tsx",
    "./typings/globals/js-cookie/index.d.ts",
    "./typings/globals/moment-node/index.d.ts",
    "./typings/globals/react-dom/index.d.ts",
    "./typings/globals/react-router/history/index.d.ts",
    "./typings/globals/react-router/index.d.ts",
    "./typings/globals/react/index.d.ts",
    "./typings/index.d.ts",
    "./custom.d.ts"
  ],
  "filesGlob": [
    "./src/**/*.ts",
    "./src/**/*.tsx",
    "./typings/**/*.ts",
    "./custom.d.ts"
  ],
  "atom": {
    "rewriteTsconfig": true
  }
}
